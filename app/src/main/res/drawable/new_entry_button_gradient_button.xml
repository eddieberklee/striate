<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <item>
    <shape android:shape="rectangle">
      <gradient
        android:angle="270"
        android:type="linear"
        />
      <corners android:radius="999dp" />
    </shape>
  </item>

  <item>
    <shape android:shape="rectangle">
      <solid />
      <stroke
        android:width="3dp"
        android:color="@color/transparent" />
      <corners android:radius="999dp" />
    </shape>
  </item>

</layer-list>